{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    },
    "@layouts/*": [
      "src/layouts/*"
    ],
    "@assets/*": [
      "src/assets/*"
    ],
    "@/utils/*": [
      "src/utils/*"
    ],
    "@components/*": [
      "src/components/*"
    ],

  }
}
